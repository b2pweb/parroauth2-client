{
    "name": "b2p/parroauth2-client",
    "description": "B2P OAuth 2 client implementation",
    "type": "library",
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packagist.b2pweb.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Parroauth2\\Client\\": "src"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "b2p/bdf-config": "~1.0",
        "b2p/bdf-session": "~1.0",
        "b2p/kangaroo": "master"
    },
    "extra": {
        "branch-alias": {
            "dev-dev": "1.2.x-dev"
        }
    },

    "require-dev": {
        "b2p/bdf-phpunit": "~1.0"
    }
}