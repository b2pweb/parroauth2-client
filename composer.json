{
  "name": "b2p/parroauth2-client",
  "description": "B2P OAuth 2 client implementation",
  "type": "library",
  "license": "proprietary",
  "config": {
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.b2pweb.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Parroauth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Parroauth2\\Client\\Tests\\": "tests/lib/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "b2p/kangaroo": "~1.0",
    "php-http/client-implementation": "~1.0",
    "php-http/client-common": "~1.0",
    "php-http/message": "~1.8",
    "spomky-labs/base64url": "~2.0",
    "web-token/jwt-signature": "~1.3",
    "web-token/jwt-checker": "~1.3"
  },
  "require-dev": {
    "b2p/bdf-phpunit": "~1.0",
    "guzzlehttp/psr7": "~1.0",
    "lcobucci/jwt": "^3.2",
    "firebase/php-jwt": "~2.2",
    "php-http/curl-client": "~1.0",
    "bshaffer/oauth2-server-php": "~1.11",
    "php-http/mock-client": "~1.3",
    "web-token/jwt-key-mgmt": "~1.3"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "scripts": {
    "tests": "phpunit"
  }
}
